#include <bits/stdc++.h>

using namespace std;

void solve() {
    int n;
    cin >> n;
    multiset<long long> s;
    vector<int> results;

    for (int i = 0; i < n; i++) {
        long long x;
        cin >> x;

        while (s.count(x)) {
            s.erase(s.find(x));
            x += x; // Updating x to the next power of itself
        }

        s.insert(x);
        results.push_back(s.size());
    }

    for (int result : results) {
        cout << result << " ";
    }
    cout << "\n";
}

int main() {
    ios::sync_with_stdio(false);
    cin.tie(0);

    int t;
    cin >> t;
    while (t--) {
        solve();
    }

    return 0;
}
