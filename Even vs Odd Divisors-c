#include <bits/stdc++.h>
using namespace std;

int main() {
    int T;
    cin >> T; // Read the number of test cases

    while (T--) {
        int N;
        cin >> N; // Read each integer N

        int evenDivisors = 0, oddDivisors = 0;

        // Find and count the even and odd divisors
        for (int i = 1; i <= N; ++i) {
            if (N % i == 0) { // i is a divisor of N
                if (i % 2 == 0) {
                    ++evenDivisors;
                } else {
                    ++oddDivisors;
                }
            }
        }

        // Output the result based on the counts of even and odd divisors
        if (evenDivisors > oddDivisors) {
            cout << 1 << endl;
        } else if (evenDivisors == oddDivisors) {
            cout << 0 << endl;
        } else {
            cout << -1 << endl;
        }
    }

    return 0;
}
